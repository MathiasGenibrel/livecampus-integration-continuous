{
  "name": "apps-web-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/apps/web-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/apps/web-api",
        "format": ["cjs"],
        "main": "packages/apps/web-api/src/main.ts",
        "tsConfig": "packages/apps/web-api/tsconfig.app.json",
        "thirdParty": false,
        "generatePackageJson": true,
        "assets": [
          "packages/apps/web-api/src/assets",
          {
            "glob": "Dockerfile",
            "input": "packages/apps/web-api",
            "output": "/"
          }
        ],
        "esbuildOptions": {
          "sourcemap": true,
          "external": ["aws-sdk", "nock", "mock-aws-s3"],
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "production": {
          "esbuildOptions": {
            "sourcemap": false
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "apps-web-api:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "apps-web-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/apps/web-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/apps/web-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
